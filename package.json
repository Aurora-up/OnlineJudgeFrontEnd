{
  "name": "onlinejudgefrontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "codegen": "openapi --input http://localhost:3000/v3/api-docs --output ./apis --client axios",
    "start:lsp:server:python": "npm run start:server:python --workspace lsp/server",
    "start:lsp:server:rust": "npm run start:server:rust --workspace lsp/server",
    "start:lsp:server:java": "npm run start:server:java --workspace lsp/server",
    "start:lsp:server:cpp": "npm run start:server:cpp --workspace lsp/server",
    "start:lsp:server:c": "npm run start:server:c --workspace lsp/server"
  },
  "dependencies": {
    "@codingame/monaco-vscode-api": "~2.0.0",
    "@codingame/monaco-vscode-configuration-service-override": "~2.0.0",
    "@codingame/monaco-vscode-cpp-default-extension": "~2.0.0",
    "@codingame/monaco-vscode-editor-api": "~2.0.0",
    "@codingame/monaco-vscode-editor-service-override": "~2.0.0",
    "@codingame/monaco-vscode-files-service-override": "~2.0.0",
    "@codingame/monaco-vscode-java-default-extension": "~2.0.0",
    "@codingame/monaco-vscode-json-default-extension": "~2.0.0",
    "@codingame/monaco-vscode-keybindings-service-override": "~2.0.0",
    "@codingame/monaco-vscode-language-pack-zh-hans": "~2.0.0",
    "@codingame/monaco-vscode-python-default-extension": "~2.0.0",
    "@codingame/monaco-vscode-rust-default-extension": "~2.0.0",
    "@codingame/monaco-vscode-textmate-service-override": "~2.0.0",
    "@codingame/monaco-vscode-theme-defaults-default-extension": "~2.0.0",
    "@codingame/monaco-vscode-theme-service-override": "~2.0.0",
    "@popperjs/core": "^2.11.8",
    "@vicons/ionicons5": "^0.12.0",
    "animate.css": "^4.1.1",
    "axios": "^1.6.8",
    "cal-heatmap": "^4.2.4",
    "dayjs": "^1.11.10",
    "echarts": "^5.4.3",
    "element-plus": "^2.7.2",
    "js-base64": "^3.7.7",
    "less": "^4.2.0",
    "md-editor-v3": "^4.11.0",
    "mitt": "^3.0.1",
    "monaco-editor": "npm:@codingame/monaco-vscode-editor-api@>=2.0.0 <2.1.0",
    "monaco-languageclient": "8.1.0",
    "pinia": "^2.1.7",
    "tdesign-icons-vue-next": "^0.2.2",
    "tdesign-vue-next": "^1.8.0",
    "undraw-ui": "^1.0.7",
    "v-calendar": "^3.1.2",
    "vscode": "npm:@codingame/monaco-vscode-api@>=2.0.0 <2.1.0",
    "vue": "^3.3.11",
    "vue-echarts": "^6.6.8",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@codingame/esbuild-import-meta-url-plugin": "~1.0.2",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.19.3",
    "@types/vscode": "^1.87.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.0",
    "editorconfig": "~2.0.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "java-parser": "^2.3.0",
    "naive-ui": "^2.37.3",
    "npm-run-all2": "^6.1.1",
    "openapi-typescript-codegen": "^0.29.0",
    "prettier": "^3.2.5",
    "prettier-plugin-java": "^2.6.0",
    "prettier-plugin-rust": "^0.1.9",
    "shx": "~0.3.4",
    "typescript": "~5.3.0",
    "vfonts": "^0.0.3",
    "vite": "^5.0.10",
    "vue-tsc": "^1.8.25"
  },
  "workspaces": [
    "lsp/server",
    "lsp/client"
  ],
  "overrides": {
    "monaco-editor": "npm:@codingame/monaco-vscode-editor-api@>=2.0.0 <2.1.0"
  },
  "resolutions": {
    "monaco-editor": "npm:@codingame/monaco-vscode-editor-api@>=2.0.0 <2.1.0"
  }
}
